@model MaxService.Models.Mantenimiento.Login
<div class="row-1 my-4">
    <h1 class="text-center text-dark px-3">Login</h1>
</div>

<form asp-controller="Administrador" asp-action="MarcaVehiculo">
    <div class="container">
        <label asp-for="Email" class="control-label"></label>
        <input asp-for="Email" class="form-control"style="background: #E0F9F6;
    border: 2px solid #000000;
    box-shadow: 0px 4px 4px rgb(0 0 0 / 25%);
    border-radius: 10px;"/>
    </div>
     <div class="container">
        <label asp-for="Password" class="control-label"></label>
        <input type="password" asp-for="Password" class="form-control" style="background: #E0F9F6;
    border: 2px solid #000000;
    box-shadow: 0px 4px 4px rgb(0 0 0 / 25%);
    border-radius: 10px;"/>
    </div>

    <div class="container my-2">
        <button type="submit" class="btn btn-primary" style="margin-bottom: 25%;">&nbsp;Entrar &nbsp;&nbsp; <img src="~/img/Login.png" alt="LOGO" width="30"> </button> 
    </div>           
</form>