@model MaxService_1.Models.Producto.MedidaLlanta

<form class="text-dark" action="" method="post">
    <div class="form-group">
        <label asp-for="Ancho">Ancho</label>
        <input type="number" required asp-for="Ancho" class="form-control" placeholder="ingresar Ancho de la llanta...">
    </div>

    <div class="form-group">
        <label asp-for="Diametro">Diametro</label>
        <input type="number" required asp-for="Diametro" class="form-control" placeholder="ingresar Diametro de la llanta...">
    </div>

    <div class="form-group">
        <label asp-for="Perfil">Perfil</label>
        <input type="number" required asp-for="Perfil" class="form-control" placeholder="ingresar Perfil de la llanta...">
    </div>

    <div class="form-group">
        <label asp-for="MmCocada">Milimetro de Cocada</label>
        <input type="number" required asp-for="MmCocada" class="form-control" placeholder="ingresar el milimetro de cocada de la llanta...">
    </div>

    <button class="btn btn-primary">Registrar</button>
</form>