@model MaxService_1.Models.Producto.Llanta

<head>
	<link rel="stylesheet" href="~/css/estilo.css">
</head>

<h1>Registro de Llantas</h1>

<form class="text-dark" action="" method="post" style="margin-bottom: 25%;">
    <div class="form-group">
        <label asp-for="Precio">Precio</label>
        <input asp-for="Precio" required class="form-control" placeholder="ingresar Precio de la llanta...">
    </div>

    <div class="form-group">
        <label asp-for="Stock">Stock</label>
        <input asp-for="Stock" required class="form-control" placeholder="ingresar Stock ...">
    </div>

    <div class="form-group">
        <label asp-for="DetalleLlanta">Detalle de la llanta</label>
        <select asp-for="DetalleLlantaId" required asp-items="ViewBag.DLL" class="form-control"></select>
    </div>

    <div class="form-group">
        <label asp-for="Vehiculo">Vehiculo</label>
        <select asp-for="VehiculoId" required asp-items="ViewBag.V" class="form-control">  </select>
    </div>

    <button class="btn btn-primary">Registrar</button>
</form>