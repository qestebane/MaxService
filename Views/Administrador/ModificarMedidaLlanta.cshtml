@model MaxService_1.Models.Producto.MedidaLlanta

<h1>Editar Medida de la Llanta</h1>

<form action="" method="post">
    <input type="hidden" asp-for="MedidaLlantaId" />

    <div class="form-group">
        <label asp-for="Ancho">Ancho</label>
        <input type="number" required asp-for="Ancho" class="form-control" >
    </div>

    <div class="form-group">
        <label asp-for="Diametro">Diametro</label>
        <input type="number" required asp-for="Diametro" class="form-control" >
    </div>

    <div class="form-group">
        <label asp-for="Perfil">Perfil</label>
        <input type="number" required asp-for="Perfil" class="form-control">
    </div>

    <div class="form-group">
        <label asp-for="MmCocada">Milimetro de Cocada</label>
        <input type="number" required asp-for="MmCocada" class="form-control">
    </div>

    <button class="btn btn-primary">Editar</button>
</form>